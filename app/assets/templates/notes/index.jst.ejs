<div class="middle-column">
  <h5 class="notes-index-header">
    <div class="inner-notes-index-header">
      <span class="glyphicon glyphicon-list-alt"></span>
      <span class="notes-index-title">
        Notes
      </span>
    </div>
  </h5>

  <ul class="nav notes-index">
    <% notes.each(function(note) { %>
      <li class="note-preview" data-id="<%= note.id %>">
        <span class="note-preview-title">
          <%= note.escape("title") %>
        </span>
        <div class="note-preview-contents">
          <%= note.preview() %>
        </div>
      </li>
    <% }) %>
  </ul>
</div>

<div class="show-area">
</div>